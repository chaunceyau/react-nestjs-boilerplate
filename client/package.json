{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@babel/helper-compilation-targets": "^7.8.4",
    "@reach/router": "^1.3.1",
    "apollo-boost": "^0.4.7",
    "awesome-typescript-loader": "^5.2.1",
    "axios": "^0.19.2",
    "cuid": "^2.1.8",
    "js-cookie": "^2.2.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hook-form": "^4.9.6",
    "react-scripts": "^3.4.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "precodegen": "apollo client:download-schema 'src/modules/api/schema.json' --endpoint=http://localhost:4000/graphql ",
    "codegen": "apollo client:codegen src/modules/api/index.d.ts --localSchemaFile='src/modules/api/schema.json' --includes='src/**/*' --target typescript --outputFlat",
    "tgen": "yarn precodegen && yarn codegen"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.13",
    "@storybook/addon-links": "^5.3.13",
    "@storybook/addons": "^5.3.13",
    "@storybook/preset-create-react-app": "^1.5.2",
    "@storybook/react": "^5.3.13",
    "@testing-library/react": "^9.4.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/user-event": "^7.1.2",
    "@types/js-cookie": "^2.2.5",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "prisma-client-lib": "^1.34.10",
    "typescript": "~3.7.2"
  }
}
